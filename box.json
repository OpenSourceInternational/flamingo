{
  "alias": "flamingo.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "directories": [
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "analog/analog/examples",
        "analog/analog/tests",
        "athari/yalinqo/Tests",
        "phpoffice/phpspreadsheet/samples",
        "psr/log/Psr/Log/Test",
        "sabre/uri/tests",
        "sabre/xml/bin",
        "sabre/xml/tests",
        "symfony/yaml/Tests",
        "nategood/commando/examples",
        "nategood/commando/tests",
        "kevinlebrun/colors.php/tests"
      ],
      "in": "vendor"
    },
    {
      "name": "DefaultConfiguration.yaml",
      "in": "bin"
    },
    {
      "name": "AdditionalConfiguration.yaml",
      "in": "bin"
    }
  ],
  "compression": "GZ",
  "main": "bin/flamingo",
  "output": "flamingo.phar",
  "stub": true
}
