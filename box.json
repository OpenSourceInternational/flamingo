{
  "alias": "flamingo.phar",
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "symfony/yaml/Tests"
      ],
      "in": "vendor"
    },
    {
      "name": "default.yml",
      "in": "bin"
    }
  ],
  "main": "bin/main.php",
  "output": "flamingo.phar",
  "stub": true
}