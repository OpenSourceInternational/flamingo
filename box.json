{
  "alias": "flamingo.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "directories": [
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "analog/analog/examples",
        "analog/analog/tests",
        "athari/yalinqo/Tests",
        "phpoffice/phpexcel/Examples",
        "phpoffice/phpexcel/unitTests",
        "psr/log/Psr/Log/Test",
        "sabre/uri/tests",
        "sabre/xml/bin",
        "sabre/xml/tests",
        "symfony/yaml/Tests"
      ],
      "in": "vendor"
    },
    {
      "name": "default.yml",
      "in": "bin"
    }
  ],
  "main": "bin/main.php",
  "output": "flamingo.phar",
  "stub": true
}